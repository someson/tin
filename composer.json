{
    "name": "someson/tin",
    "description": "TIN (Taxpayer Identification Number) online check handler",
    "license": "MIT",
    "keywords": ["tin", "evatr", "vat"],
    "type": "library",
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-intl": "*",
        "guzzlehttp/guzzle": "^6",
        "tolerance/tolerance": "^0.4.2"
    },
    "require-dev": {
        "codeception/codeception": "^4",
        "phpstan/phpstan": "^0.12",
        "codeception/module-asserts": "^1.3",
        "codeception/module-filesystem": "^1.0"
    },
    "authors": [
        {
            "name": "someson",
            "email": "somevov4ik@gmail.com"
        }
    ],
    "suggest": {
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "Someson\\TIN\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Someson\\Tests\\Unit\\": "tests/unit/"
        }
    }
}
